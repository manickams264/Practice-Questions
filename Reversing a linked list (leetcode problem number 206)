struct ListNode* reverseList(struct ListNode* head) {
    if (head == NULL || head -> next == NULL) {
        return head;
    }    
    struct ListNode *current = head;
    struct ListNode *previous = NULL;
    while(current) {
        struct ListNode *next = current -> next;
        current -> next = previous;
        previous = current;
        current = next;
    }
    return previous;
}
