struct ListNode* mergeTwoLists(struct ListNode* list1, struct ListNode* list2) {
    struct ListNode dummy;
    struct ListNode *newNode = &dummy;
    dummy.next = NULL;
    while (list1 != NULL && list2 != NULL) {
        if(list1 -> val <= list2 -> val) {
            newNode -> next = list1;
            list1 = list1 -> next;
        }
        else {
            newNode -> next = list2;
            list2 = list2 -> next;
        }
        newNode = newNode -> next;
    }
    if(list1 != NULL) {
        newNode -> next = list1;
    }
    else {
        newNode -> next = list2;
    }
    return dummy.next;
}
